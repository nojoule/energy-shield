[sub_resource type="GDScript" id="GDScript_sq7vv"]
script/source = "@tool
extends Node3D

var rotation_speed: float = 45.0

var impact_cooldown: float = 1.0
var time_passed: float = 0.0
var initial_position: Vector3 = Vector3(0.75, 0.6, 0.0)

@onready var left_sphere: MeshInstance3D = $ShieldSphere
@onready var right_sphere: MeshInstance3D = $ShieldSphere2


func random_point_on_sphere(radius: float) -> Vector3:
	var theta = randf() * PI * 2.0
	var phi = acos(1.0 - 2.0 * randf())

	var x = radius * sin(phi) * cos(theta)
	var y = radius * sin(phi) * sin(theta)
	var z = radius * cos(phi)

	return Vector3(x, y, z)

func _process(delta: float):
	time_passed += delta
	if time_passed > impact_cooldown:
		var impact_point = left_sphere.global_position
		var random_point = random_point_on_sphere(0.5)
		left_sphere.impact(impact_point + random_point)
		impact_point = right_sphere.global_position
		right_sphere.impact(impact_point + random_point)
		time_passed -= impact_cooldown
"

