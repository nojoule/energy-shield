[sub_resource type="GDScript" id="GDScript_1dyov"]
script/source = "@tool
extends Node3D

var rotation_speed: float = 45.0
var rainbow_speed: float = 0.2
var hue: float = 0.0

@onready var sphere: MeshInstance3D = $ShieldSphere
@onready var plane: MeshInstance3D = $ShieldPlane

func _process(delta: float):
	plane.rotate(Vector3.UP, deg_to_rad(rotation_speed * delta))
	hue += rainbow_speed * delta
	hue = wrapf(hue, 0.0, 1.4)
	var brightness = 1.0
	var saturation = 0.7
	if hue > 1.0:
		brightness = clamp(1.0 - (hue - 1.0) * 10.0, 0.0, 1.0)
	if hue > 1.2:
		brightness = clamp((hue - 1.2) * 10.0, 0.0, 1.0)
		saturation = 0.0
	if hue > 1.3:
		saturation = clamp((hue - 1.3) * 10.0, 0.0, 1.0)

	var color = Color.from_hsv(clamp(hue, 0.0, 1.0), saturation, brightness)
	if Engine.is_editor_hint():
		sphere.get_active_material(0).set_shader_parameter(\"_color_shield\", color)
	else:
		sphere.update_material(\"_color_shield\", color)
	color = Color.from_hsv(1.0 - clamp(hue, 0.0, 1.0), saturation, brightness)
	if Engine.is_editor_hint():
		plane.get_active_material(0).set_shader_parameter(\"_color_shield\", color)
	else:
		plane.update_material(\"_color_shield\", color)
"

