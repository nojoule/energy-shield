script/source = "@tool
extends Node3D

var rotation_speed: float = 45.0

var amplitude: float = 0.5
var frequency: float = 0.2
var time_passed: float = 0.0
var initial_position: Vector3 = Vector3(0.75, 0.6, 0.0)

@onready var sphere: MeshInstance3D = $ShieldSphere
@onready var plane: MeshInstance3D = $ShieldPlane
@onready var moving_box: MeshInstance3D = $EnvironmentMovingPlatform

func _process(delta: float):
	if moving_box:
		time_passed += delta
		var y_offset = amplitude * sin(time_passed * frequency * TAU)
		moving_box.transform.origin = initial_position + Vector3(0, y_offset, 0)

	plane.rotate(Vector3.UP, deg_to_rad(rotation_speed * delta))
"

