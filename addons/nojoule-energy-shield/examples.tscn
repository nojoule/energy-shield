[sub_resource type="GDScript" id="GDScript_ebi6u"]
script/source = "@tool
extends Node3D


# --------------- Signals ----------------------
#signal
# ---------------- Enums -----------------------
#enum
# -------------- Constants ---------------------
#const
# --------------- Exports ----------------------
## The shield node that this script affects.
@export var plane_node: MeshInstance3D = null
## Max amount of rain drops in each burst. Accual value is randomized 
## value that has a max of this value.
@export var drops_in_burst: int = 20
## Max time between drops in each burst. Accual value is randomized value
## that has a max of this value.
@export var max_time: float = 0.05
## The change of a batch of rain drops to start falling on the shield 
## each frame.
@export_range(0.001, 1.0, 0.001) var rain_burst_chance: float = 0.5
# -------------- Variables ---------------------
var plane_size: Vector2 = Vector2.ZERO
var raining := false
# --------------- Onready ----------------------
#@onready


# ------------ Process Functions -----------------
func _ready() -> void:
	plane_size.x = plane_node.mesh.size.x * plane_node.scale.x
	plane_size.y = plane_node.mesh.size.y * plane_node.scale.y


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	if not raining and randf() < rain_burst_chance:
		raining = true
		_rain()


#func _physics_process(delta: float) -> void:
	#pass


# ------------ Private Functions -----------------
func _rain() -> void:
	var rain: int = randi_range(drops_in_burst * 0.1, drops_in_burst)
	for drop in rain:
		await get_tree().create_timer(randf_range(max_time * 0.001, max_time)).timeout
		var offset: Vector3 = Vector3.ZERO
		offset.x = randf_range(-plane_size.x, plane_size.x)
		offset.y = randf_range(-plane_size.y, plane_size.y)
		var drop_pos: Vector3 = plane_node.global_position + offset
		plane_node.impact(drop_pos, null)
	raining = false


# ------------ Public Functions -----------------
#func example() -> void:
	#pass
"

