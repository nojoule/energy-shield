			#offset.y = randf_range(-plane_size.y, plane_size.y)
			#var drop_pos: Vector3 = plane_node.global_position + offset
			#plane_node.impact(drop_pos, null)


# ------------ Private Functions -----------------
#func _example() -> void:
	#pass


# ------------ Public Functions -----------------
#func example() -> void:
	#pass
"

