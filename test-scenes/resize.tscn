[sub_resource type="GDScript" id="GDScript_mhf7u"]
script/source = "extends Node3D

var cumulative_time: float = 0.0

@onready var shield_sphere: NJEnergyShield = $ShieldSphere
@onready var shield_sphere_2: NJEnergyShield = $ShieldSphere2
@onready var shield_sphere_3: NJEnergyShield = $ShieldSphere3

var rotation_speed: float = 45.0

var impact_cooldown: float = 5.0
var time_passed: float = 5.0
var initial_position: Vector3 = Vector3(0.75, 0.6, 0.0)



func random_point_on_sphere(radius: float) -> Vector3:
	var theta = randf() * PI * 2.0
	var phi = acos(1.0 - 2.0 * randf())

	var x = radius * sin(phi) * cos(theta)
	var y = radius * sin(phi) * sin(theta)
	var z = radius * cos(phi)

	return Vector3(x, y, z)

func _process(delta: float):
	time_passed += delta
	if time_passed > impact_cooldown:
		var impact_point = shield_sphere.global_position
		var random_point = random_point_on_sphere(0.5)
		shield_sphere.impact(impact_point + random_point * max(shield_sphere.scale.x, shield_sphere.scale.y, shield_sphere.scale.z))
		impact_point = shield_sphere_2.global_position
		shield_sphere_2.impact(impact_point + random_point * max(shield_sphere_2.scale.x, shield_sphere_2.scale.y, shield_sphere_2.scale.z))
		impact_point = shield_sphere_3.global_position
		shield_sphere_3.impact(impact_point + random_point * max(shield_sphere_3.scale.x, shield_sphere_3.scale.y, shield_sphere_3.scale.z))
		time_passed -= impact_cooldown
"

